- local/
  ! Time.now.strftime("%l:%M:%S %p %Z")
- time zone/
  ! zone = args[0]
  ! if ! zone
  !   return ActiveSupport::TimeZone.us_zones.map(&:name).map{|o| "- #{o}/\n"}.join('')
  ! end
  !
  ! zone.sub! /^\| /, ''
  ! t = Time.now.in_time_zone ActiveSupport::TimeZone.new(zone)
